<%= render 'shared/header', header: 'Register' %>
<%= render 'shared/errors', obj: @user %>

<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <br>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.label :password_confirm %>
  <%= f.password_field :password_confirmation %>
  <br>
  <%= f.submit 'Register' %>
<% end %>